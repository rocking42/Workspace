<div class="time_table">
  <% count = 0 %>
  <% org.user_tasks.each do |gp| %>
    <% if gp.task == time_stuff && timer.to_date == gp.end_date %>
      <p><%= link_to gp.task, root_url if count < 1 %></p>
      <p><%= gp.end_date.to_s(:short) %>
         <%#= link_to gp.task, group_group_project_path(gp.group, gp) %></p>
      <% count += 1 %>
    <% end %>
  <% end %>
</div>
