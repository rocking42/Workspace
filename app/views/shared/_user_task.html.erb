<div class="center">
  <% group_project.user_tasks.each do |user_task| %>
    <div class="width row">
      <div class="col s12 m12">
        <div class="card blue darken-2 darken-1 hoverable">
          <div class="card-content white-text">
            <span class="card-title"><p><%= user_task.task %></p></span>
            <p>End Date: <%= user_task.end_date.to_s(:long) %></p>
          </div>
          <div class="card-action">
            <%= link_to user_task.user, class: "profile-spacing" do %>
              <%= cl_image_tag("#{user_task.user.image}.jpg", class: "image-small")  %>
            <% end %>
          </div>
        </div>
      </div>
    </div>
  <% end %>
</div>
