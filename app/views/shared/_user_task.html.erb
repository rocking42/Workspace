<div class="center">
  <% group_project.user_tasks.each do |user_task| %>
    <% if !user_task.completed == true %>
    <div class="width row">
      <div class="col s12 m12">
        <div class="card hoverable">
          <div class="card-content">
            <span class="card-title "><p><%= link_to truncate(user_task.task, length: 25, separator: " "), user_task_path(user_task.group_project, user_task.id) %></p></span>
            <p>End Date: <%= user_task.end_date.to_s(:long) %></p>
          </div>
          <div class="card-action">
            <%= link_to user_task.user, class: "profile-spacing" do %>
              <%= cl_image_tag("#{user_task.user.image}.jpg", class: "image-small")  %>
            <% end %>
          </div>
        </div>
      </div>
    </div>
  <% end %>
  <% end %>
</div>
