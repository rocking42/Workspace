<div class="row">
  <div class="col s1">
    <p>
      <%= link_to group_post.user do %>
        <%= cl_image_tag("#{group_post.user.image}.jpg", class: "image-small")  %>
      <% end %>
    </p>
  </div>
  <div class="col s11">
    <br>
    <p><strong><%= group_post.user.name %></strong> <%= time_ago_in_words(group_post.created_at) %> ago</p>
    <p><%= group_post.post.html_safe %></p>
  </div>
</div>
