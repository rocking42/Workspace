<div class="width row">
  <div class="col s12 m12">
    <div class="card blue darken-2 darken-1 hoverable">
      <div class="card-content white-text">
        <span class="card-title"><p><%= link_to truncate(group_project.task.capitalize, length: 25, separator: " "), group_group_project_path(group_project.group, group_project), class: "blue-text text-lighten-5" %></p></span>
      </div>
      <div class="card-action">
        <% group_project.users.uniq.each.with_index do |user, i| %>
          <% if user.image && i <= 10  %>
            <%= link_to user, class: "profile-spacing" do %>
              <%= cl_image_tag("#{user.image}.jpg", class: "image-small")  %>
            <% end %>
          <% end %>
        <% end %>
        <% if group_project.users.count == 0 %>
          <p style="height:33px;"></p>
        <% end %>
        <% if group_project.users.count < 7 %>
          <p style="height:3px"></p><p style="height:3px"></p>
        <% end %>
      </div>
    </div>
  </div>
</div>
